  <!-- EN: body purist squeamish -->
  <ProsthophobeUnhappy.stages.body_purist_squeamish.label>purista corporal profanad{PAWN_gender ? o : a}</ProsthophobeUnhappy.stages.body_purist_squeamish.label>
