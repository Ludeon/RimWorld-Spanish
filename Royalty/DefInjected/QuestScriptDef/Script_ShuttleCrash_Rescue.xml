    <li>pawnLabelSingleOrPlural(civilians_count>=2)->[pawnLabelPlural]</li>
    <li>pawnLabelSingleOrPluralDef(civilians_count==1)->el [pawnLabel]</li>
    <li>pawnLabelSingleOrPluralDef(civilians_count>=2)->los [pawnLabelPlural]</li>
    <li>crashedLanded(civilians_count==1)->estrelló</li>
    <li>crashedLanded(civilians_count>=2)->estrellaron</li>
