  <EmbracedTheVoid.stages.embraced_the_void.label>acepté al vacío</EmbracedTheVoid.stages.embraced_the_void.label>
  <!-- EN: This is just the beginning. -->
  <EmbracedTheVoid.stages.embraced_the_void.description>Esto es solo el comienzo.</EmbracedTheVoid.stages.embraced_the_void.description>
  
  <!-- EN: healed me -->
  <HealedMe.stages.healed_me.label>me curaron</HealedMe.stages.healed_me.label>
  
  <!-- EN: {0} rambling -->
  <HeardInhumanRambling.stages.rambling.label>{0} está delirando</HeardInhumanRambling.stages.rambling.label>
  <!-- EN: {OTHERPAWN_nameDef} is talking nonsense. Is {OTHERPAWN_pronoun} insane? -->
